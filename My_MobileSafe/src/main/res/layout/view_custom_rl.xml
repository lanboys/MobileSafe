<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/root_custom_rl"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/call_locate_orange">

    <TextView

        android:id="@+id/tv_custom_big_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="归属地提示框风格"
        android:textSize="20dp"/>

    <TextView
        android:id="@+id/tv_custom_small_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tv_custom_big_title"
        android:text="归属地提示框风格"
        android:textSize="18dp"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:src="@drawable/default_icon"/>


</RelativeLayout>